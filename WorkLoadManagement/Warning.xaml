<Window x:Class="WorkLoadManagement.Warning"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WorkLoadManagement"
        mc:Ignorable="d"
        Title="Warning" Height="150" Width="250">
    <Grid>
        <TextBlock Text="{Binding ErrorMsg}" FontSize="15" HorizontalAlignment="Center" Margin="0,15,0,0"></TextBlock>
        <Button Name="message" Content="OK" HorizontalAlignment="Center" Margin="96,68,96.333,20.667" ></Button>
        
    </Grid>
</Window>
